package com.panguso.datatransfer.conf;


/**
 * 下载信息类
 * @author liubing
 *
 */
public class DownloadFileInfo {

	private String remotePath;		//待下载的文件路径（FTP服务器上的路径）
	private String localPath;		//下载后存放的路径（本地路径）
	private int downloadres = 0;	//下载结果状态
	
	public String getRemotePath() {
		return remotePath;
	}
	public void setRemotePath(String remotePath) {
		this.remotePath = remotePath;
	}
	public String getLocalPath() {
		return localPath;
	}
	public void setLocalPath(String localPath) {
		this.localPath = localPath;
	}
	public int getDownloadres() {
		return downloadres;
	}
	public void setDownloadres(int downloadres) {
		this.downloadres = downloadres;
	}
}
